  "keyFeatures": [
    "Terminal multiplexer with 9 independent workspaces",
    "BSP tiling with preselection and manual split control",
    "Vim-style copy mode with 10,000 line scrollback",
    "Tape scripting DSL for automation and CI/CD",
    "Session persistence with daemon mode (attach/detach)",
    "SSH server mode for remote terminal multiplexing",
    "Web terminal mode with WebGL rendering",
    "Customizable keybindings via TOML configuration",
    "Mouse support with drag, resize, and click interactions",
    "Built with Bubble Tea v2 and Lipgloss v2"
  ],
  "publicApiSurface": [
    "tuios - Launch interactive terminal manager",
    "tuios new [session] - Create persistent session",
    "tuios attach [session] - Attach to session",
    "tuios ls - List sessions",
    "tuios ssh - Run SSH server",
    "tuios tape play <file> - Run tape automation",
    "tuios config edit - Edit configuration",
    "tuios keybinds list - View keybindings",
    "tuios send-keys <keys> - Send keys to session",
    "tuios run-command <cmd> - Execute remote commands",
    "tuios list-windows - List windows in session",
    "tuios-web - Web terminal server binary"
  ]
